#define CNT_NOTES(patt, patt2)                         \
 curve pattern patt {				       \
   sql = SELECT version_name, release_date, count(*)   \
       	 	FROM "Notes info"                      \
       	 	WHERE standardized_name = patt2        \
		AND release_date > '2002-01-01'        \
		GROUP BY version_name,                 \
		      release_date,                    \
		      standardized_name                \
		ORDER BY release_date;                 \
 }

