  COUNT_DEF_GRP('linux-2.4.1', "LockIntr" , 'LockIntr' ) \
  COUNT_DEF_GRP('linux-2.4.1', "Free"     , 'Free'     ) \
  COUNT_DEF_GRP('linux-2.4.1', "Size"     , 'Size'     )

}

